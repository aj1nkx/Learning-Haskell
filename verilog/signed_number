#! /usr/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x22d2920 .scope module, "signed_number" "signed_number" 2 1;
 .timescale 0 0;
v0x22d2a10_0 .var "a", 31 0;
    .scope S_0x22d2920;
T_0 ;
    %movi 8, 4660, 32;
    %set/v v0x22d2a10_0, 8, 32;
    %vpi_call 2 7 "$display", "Current Value of a = %h", v0x22d2a10_0;
    %movi 8, 4294962636, 32;
    %set/v v0x22d2a10_0, 8, 32;
    %vpi_call 2 9 "$display", "Current Value of a = %h", v0x22d2a10_0;
    %movi 8, 3735928559, 32;
    %set/v v0x22d2a10_0, 8, 32;
    %vpi_call 2 11 "$display", "Current Value of a = %h", v0x22d2a10_0;
    %movi 8, 559038737, 32;
    %set/v v0x22d2a10_0, 8, 32;
    %vpi_call 2 13 "$display", "Current Value of a = %h", v0x22d2a10_0;
    %delay 10, 0;
    %vpi_call 2 14 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
